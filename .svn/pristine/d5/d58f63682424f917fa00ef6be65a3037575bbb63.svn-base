.directive('vnVisionWorkflowsSent', function() {
    return {
      restrict: 'E',
      templateUrl: 'apps/visiontasks/vnvisionworkflowssent.html',
      scope: { //DCB if you remove this scope there's a $parent.$parent below that should then be just $scope
      	allUsers: "=",
      	scroller: "@"
      },
