<vn-css vn-id="vnuserchips" vn-css-file="apps/components/chips/user/vnuserchips.css"></vn-css>
<div class="vision-user-chips">
	<vn-chips
		vn-placeholder='' 
		vn-secondary-placeholder='' 
		vn-template='vn-user-chips-detail'
		collection='usersCollection' 
		ng-model='ngModel' 
		add-chips-handler="addChipsHandler()" 
		md-title='title'  
		md-subtitle='subtitle' 
		md-thumbnail='thumbnailUrl' 
		md-item='userItems'></vn-chips>
</div>
