
	
	
	<form id="workflowDesigner">
	
		<div class="configuration-screen">
			<div class="workflow-component">
				<div id="initialComponentContainer" class="workflow-query-part">
					<select id="fields"></select>
				</div>
				<div id="conditionContainer" class="workflow-query-part" style="display:none;">
					<select id="condition"></select>
				</div>
				<div id="valueContainer" class="workflow-query-part" style="display:none;">
					<select id="value"></select>
				</div>
				<div id="attributeContainer" class="workflow-query-part" style="display:none;">
					<select id="constant"></select>
				</div>
				<div id="picklistContainer" class="workflow-query-part" style="display:none;">
					<inps-form-field:select
						items="${referenceDataMap[pickListName]}"
						label="sscd"
						value=""
						name="dcsdd"
						labelName="READ_TERM"
						valueField="id"
						labelField="name"
						id="picklist"
						hideLabelArea="true"
						/>
				</div>
				<div id="buttonContainer" class="workflow-query-part" style="display:none;">
					<input id="done" type="button" value="Done" />
				</div>
			</div>
			<div class="lozengePark">
			
			</div>
		</div>
	</form>
