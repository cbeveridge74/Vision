#navigation .home {
	font-size: 16px;
	text-align: right;
	font-weight: bold;
}

#testMenu{
	display: none;
	position: absolute;
}

#testMenu a{
	color: #000000 !important;
}

.lozengeLabel{
	text-align: left;
	position: absolute;
	width: 120px;
	text-align: center;
	font-weight: bold;
	font-size: large;
	color: #555555
}

a:link {text-decoration: none;color: black;} 
a:visited {text-decoration: none;color: black;} 
a:active {text-decoration: none;color: black;} 
a:hover {text-decoration: none; color: black;}

li {
	list-style: none;
	margin: 0px;
	padding: 0px;
}

.inps_class_nurse_64_icon{
	background:no-repeat url('img/nurse_icon_64.png') !important;
}

.inps_class_community_64_icon{
	background:no-repeat url('../images/community.png') !important;
}

.inps_class_blood_48_icon{
	background:no-repeat url('img/blood_icon_48.png') !important;
}

.inps_class_new_64_icon{
	background:no-repeat url('../formbuilder/new-icon.png') !important;
}

.inps_class_existing_64_icon{
	background:no-repeat url('../formbuilder/existing-icon.png') !important;
}

/* 	Full width and Triple width values are making the screen size slightly
	larger than what is available to the ipad.  Buttons are slightly offscreen
	and a border is only being displayed on the right side.
	These hacks are put in just now for wales and NVUG demos, but this should
	be addressed properly.  Will not work where there are more than
	three buttons within the frame at once.
*/
@media only screen and (min-device-width: 768px) and (max-device-width: 1024px) {
	div.section {
		border: none;
	}
	
	div.row div.entity input[type=button]:nth-child(3){
		margin-right: 3em;
	}
}
