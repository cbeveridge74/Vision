.ui-numeric { display: inline-block; zoom: 1 !important; *display: inline !important; }
.ui-numeric input[type=text] { border: none; text-align: right; margin: 0px; vertical-align: top; }
.ui-numeric-currency { display: inline-block; padding: 0px 2px; }
.ui-numeric-buttons { display: inline-block; vertical-align: top; height: 1.5em; width:1.5em; margin-left: 0px; zoom: 1 !important; *display: inline !important; }
.ui-numeric-buttons .ui-button { margin: 0px; width: 1.55em; height: 0.9em; vertical-align: top; }
.ui-numeric-buttons span { padding:0; margin:0; }