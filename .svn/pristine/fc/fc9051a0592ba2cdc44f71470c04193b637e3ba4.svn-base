function FormBuilderState(){
}

// Screens
FormBuilderState.mainScreenInstance 			= new MainScreen();
FormBuilderState.configurationScreenInstance 	= new ConfigurationScreen();
FormBuilderState.existingFormsInstance 			= new ExistingForms();
FormBuilderState.landingScreenInstance 			= new LandingScreen();
FormBuilderState.saveScreenInstance 			= new SaveScreen();
FormBuilderState.settingsScreenInstance 		= new SettingsScreen();

// Component settings
FormBuilderState.textComponentSettingsInstance 		= new TextComponentSettings();
FormBuilderState.dropDownComponentSettingsInstance 	= new DropDownComponentSettings();
FormBuilderState.numericComponentSettingsInstance 	= new NumericComponentSettings();
FormBuilderState.searchComponentSettingsInstance 	= new SearchComponentSettings();
FormBuilderState.hiddenComponentSettingsInstance 	= new HiddenComponentSettings();
FormBuilderState.dateComponentSettingsInstance 		= new DateComponentSettings();

FormBuilderState.componentSettings = [ FormBuilderState.textComponentSettingsInstance,
                                       FormBuilderState.dropDownComponentSettingsInstance,
                                       FormBuilderState.numericComponentSettingsInstance,
                                       FormBuilderState.searchComponentSettingsInstance,
                                       FormBuilderState.hiddenComponentSettingsInstance,
                                       FormBuilderState.dateComponentSettingsInstance ];

FormBuilderState.componentClassNames = ".textinputcomponent,.dropdowncomponent,.numericcomponent,.searchcomponent,.hiddencomponent,.datecomponent";

// Models
FormBuilderState.entityModel = new EntityModel();
FormBuilderState.formModel = new FormModel();
FormBuilderState.fieldModel = new FieldModel();
