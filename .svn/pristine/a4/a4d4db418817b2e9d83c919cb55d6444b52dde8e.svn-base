<div class="vision-data_entry"
	 ng-controller="vnDataEntryController"
	 ng-init="init();">
	<vn-css vn-id="vndataentry" vn-css-file="apps/visiondataentry/vndataentry.css"></vn-css>
	<vn-css vn-id="numericjquery" vn-css-file="apps/visiondataentry/scripts/jquery-ui-numeric.css"></vn-css>
	<form id="previewForm">
		<div class="vn-row" ng-repeat="(index, row) in formData.row">
			<div class="left">
				<div ng-repeat="(rowpartindex, rowpart) in row.left" class="rowpart">
					<div ng-switch on="rowpart.type[0].text">
						<div class='vn-label'>{{rowpart.settings[0].label[0].text}}</div>
					    <input ng-switch-when="textinput" id="{{rowpart.settings[0].fieldid[0].text}}" class='input' type='text'>
						<select ng-switch-when="dropdown" id="{{rowpart.settings[0].fieldid[0].text}}" class='input'>
							<option ng-repeat="(optionindex, optionvalue) in getPicklist( rowpart.settings[0].referencedata[0].text )" value="{{ optionvalue.id }}">{{ optionvalue.label }}</option>
						</select>
					    <input ng-switch-when="numeric" 
					    id="{{rowpart.settings[0].fieldid[0].text}}" 
					    class='input numeric' 
					    type='number'
					    min='{{rowpart.settings[0].min[0].text}}'
					    max='{{rowpart.settings[0].max[0].text}}'>
					    <input ng-switch-when="date" id="{{rowpart.settings[0].fieldid[0].text}}" class='input dateinput' type='text'>
					    <div class="input units">{{ rowpart.settings[0].units[0].text }}</div>
					</div>
				</div>
			</div>
			<div class="right">
				<div ng-repeat="(rowpartindex, rowpart) in row.right" class="rowpart">
					<div ng-switch on="rowpart.type[0].text">
						<div class='vn-label'>{{rowpart.settings.label}}</div>
					    <input ng-switch-when="textinput" id="{{rowpart.settings.fieldid}}" class='input' type='text'></input>
						<select ng-switch-when="dropdown" id="{{rowpart.settings.fieldid}}" class='input'>
							<option></option>
						</select>
					    <input ng-switch-when="numeric" id="{{rowpart.settings.fieldid}}" class='input' type='text'></input>
					</div>
				</div>
			</div>
		</div>
		<input id="inps_patientdateofbirth" type="hidden" value="23-Sep-2013">
		<input id="inps_patientdateofdeath" type="hidden" value="">
		<input id="inps_systemdate" type="hidden" value="16-Dec-2014">

	</form>
</div>